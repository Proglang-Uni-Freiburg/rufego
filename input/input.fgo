package main;

type message struct {
    value int
}

type client struct {}

type handler interface {
    sendMsg(content int) int
    receiveMsg() int
}

func (c client) sendMsg(content int) int {
    return (message{ content }).value
}

func (c client) receiveMsg() int {
    return c.sendMsg(5)
}

func main() {
    _ = message{ (client{}).receiveMsg() }
}