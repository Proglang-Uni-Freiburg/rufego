package main;

type container interface {
    createConsumer(newValue int) container
    getValue() int
}

type consumer struct {
    amount int
}

func (c consumer) createConsumer(new int) container {
    return consumer{ new }
}

func (c consumer) getValue() int {
    return c.amount
}

func main() {
    _ = ((consumer{ 0 }).createConsumer(42).getValue()) + 5
}